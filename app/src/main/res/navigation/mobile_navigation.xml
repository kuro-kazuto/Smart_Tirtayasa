<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.labcnt.smarttirtayasa.ui.home.HomeFragment"
        android:label="Beranda"
        tools:layout="@layout/fragment_home"
        />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.labcnt.smarttirtayasa.ui.history.HistoryFragment"
        android:label="Riwayat"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.labcnt.smarttirtayasa.ui.chat.ChatFragment"
        android:label="Chat"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_inbox"
        android:name="com.labcnt.smarttirtayasa.ui.inbox.InboxFragment"
        android:label="Inbox"
        tools:layout="@layout/fragment_inbox" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.labcnt.smarttirtayasa.ui.account.AccountFragment"
        android:label="Akun"
        tools:layout="@layout/fragment_account" />
</navigation>